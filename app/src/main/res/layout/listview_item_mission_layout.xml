<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingHorizontal="@dimen/activity_horizontal_margin"
        android:paddingVertical="@dimen/padding_small"
>


    <CheckBox
            android:id="@+id/item_mission_ck_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:theme="@style/primary_checkbox"

    >

    </CheckBox>


    <TextView
            android:id="@+id/item_mission_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="主单号:4545655656565"
            style="@style/normal_tv_red"
            app:layout_constraintTop_toTopOf="@id/item_mission_ck_main"
            app:layout_constraintBottom_toBottomOf="@id/item_mission_ck_main"
            app:layout_constraintLeft_toRightOf="@id/item_mission_ck_main"
    />

    <com.bondex.ysl.battledore.ui.FillListView
            android:id="@+id/item_mission_recyclerview"
            android:layout_width="match_parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/item_mission_main"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/main_small"
    >

    </com.bondex.ysl.battledore.ui.FillListView>



</android.support.constraint.ConstraintLayout>