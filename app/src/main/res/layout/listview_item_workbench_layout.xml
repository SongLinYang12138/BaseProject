<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:layout_height="40dp"

        tools:ignore="MissingDefaultResource">

    <TextView
            android:id="@+id/item_work_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            app:layout_constraintTop_toTopOf="@id/item_work_rde"
            app:layout_constraintBottom_toBottomOf="@id/item_work_rde"
            style="@style/normal_tv_gray"

    />

    <com.bondex.ysl.liblibrary.ui.ReduceAddEditText

            android:id="@+id/item_work_rde"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="@dimen/main_small"
            app:layout_constraintLeft_toRightOf="@id/item_work_tv"
            app:layout_constraintBottom_toBottomOf="parent"
    >


    </com.bondex.ysl.liblibrary.ui.ReduceAddEditText>

</android.support.constraint.ConstraintLayout>